device=test
144,0,127=mpc prev
144,1,127=mpc toggle
144,2,127=mpc next
144,3,127=mpc volume -5
144,4,127=mpc volume +5
144,96,127=xdotool key "alt+e"
144,97,127=xdotool key "alt+o"
144,112,127=xdotool keydown "alt+KP_1"
144,112,0=xdotool keyup "alt+KP_1"
144,113,127=xdotool keydown "alt+KP_2"
144,113,0=xdotool keyup "alt+KP_2"
144,114,127=xdotool keydown "alt+KP_3"
144,114,0=xdotool keyup "alt+KP_3"
144,115,127=xdotool keydown "alt+KP_4"
144,115,0=xdotool keyup "alt+KP_4"
